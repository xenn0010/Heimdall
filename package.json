{
  "name": "heimdall-cli",
  "version": "0.1.0",
  "description": "CLI tool for Git automation and fast code fixes with Morph",
  "type": "module",
  "bin": {
    "heimdall": "./dist/bin/heimdall.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx src/dev.ts",
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "prepare": "npm run build"
  },
  "keywords": [
    "cli",
    "git",
    "automation",
    "morph",
    "ai",
    "code-fixes"
  ],
  "author": "Heimdall Team",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^17.2.1",
    "execa": "^9.3.0",
    "glob": "^11.0.3",
    "node-fetch": "^3.3.2",
    "openai": "^4.104.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^9.9.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heimdall-ai/heimdall-cli.git"
  },
  "bugs": {
    "url": "https://github.com/heimdall-ai/heimdall-cli/issues"
  },
  "homepage": "https://github.com/heimdall-ai/heimdall-cli#readme",
  "publishConfig": {
    "access": "public"
  }
}